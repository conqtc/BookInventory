    <bpel:import namespace="http://www.example.org/BookOrderService/" location="BookOrderService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import namespace="http://a2.waa/BookOrderArtifacts" location="BookOrderProcessArtifacts.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:partnerLinks>
        <bpel:partnerLink name="BookOrderPL" partnerLinkType="ns1:BookOrderPLT" myRole="BookOrderRole"></bpel:partnerLink>
        <bpel:partnerLink name="StudentServicePL" partnerLinkType="ns1:StudentServicePLT" partnerRole="StudentServiceRole"></bpel:partnerLink>
        <bpel:partnerLink name="BookInventoryServicePL" partnerLinkType="ns1:BookInventoryServicePLT" partnerRole="BookInventoryServiceRole"></bpel:partnerLink>
        <bpel:partnerLink name="BookInformationServicePL" partnerLinkType="ns1:BookInformationServicePLT" partnerRole="BookInformationServiceRole"></bpel:partnerLink>
    </bpel:partnerLinks>
    <bpel:variables>
        <bpel:variable name="BookOrderPLRequest" messageType="ns:orderBookRequest"></bpel:variable>
        <bpel:variable name="BookOrderPLResponse" messageType="ns:orderBookResponse"></bpel:variable>
