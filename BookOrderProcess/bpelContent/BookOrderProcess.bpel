</ns:validateStudent>
</bpel:literal></bpel:from>
                <bpel:to variable="StudentServicePLRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="BookOrderPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[studentid]]>
                    </bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="StudentServicePLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[ns0:id]]>
                    </bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="BookOrderPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[pin]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="StudentServicePLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns0:pin]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="invokeStudentService" partnerLink="StudentServicePL" operation="validateStudent" portType="ns0:StudentServicePortType" inputVariable="StudentServicePLRequest" outputVariable="StudentServicePLResponse"></bpel:invoke>
        <bpel:if name="IfStudentIsValid">
            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$StudentServicePLResponse.parameters/ns0:return="yes"]]></bpel:condition>
            <bpel:sequence>
                <bpel:assign validate="no" name="assignToValidateBook">
                    <bpel:copy>
                        <bpel:from>
                        	<bpel:literal>
<ns:validateBook xmlns:ns="http://waa.a2.bookinventory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<ns:isbn>ns:isbn</ns:isbn>
</ns:validateBook>
							</bpel:literal>
						</bpel:from>
                        <bpel:to variable="BookInventoryServicePLRequest" part="parameters"></bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from part="parameters" variable="BookOrderPLRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[isbn]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="parameters" variable="BookInventoryServicePLRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns2:isbn]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                </bpel:assign>
                <bpel:invoke name="invokeBookInventoryService" partnerLink="BookInventoryServicePL" operation="validateBook" portType="ns2:BookInventoryServicePortType" inputVariable="BookInventoryServicePLRequest" outputVariable="BookInventoryServicePLResponse"></bpel:invoke>
                <bpel:if name="IfBookNotInDb">
                    <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$BookInventoryServicePLResponse.parameters/ns2:return="yes"]]></bpel:condition>
                    <bpel:sequence>
                        <bpel:assign validate="no" name="assignToValidateBookInfo">
                            <bpel:copy>
                                <bpel:from>
                                	<bpel:literal>
<ns:getBookInfo xmlns:ns="http://waa.a2.bookinformation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<ns:isbn>ns:isbn</ns:isbn>
</ns:getBookInfo>
									</bpel:literal>
								</bpel:from>
                                <bpel:to variable="BookInformationServicePLRequest" part="parameters"></bpel:to>
                            </bpel:copy>
                            <bpel:copy>
                                <bpel:from part="parameters" variable="BookOrderPLRequest">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[isbn]]></bpel:query>
                                </bpel:from>
                                <bpel:to part="parameters" variable="BookInformationServicePLRequest">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns3:isbn]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        <bpel:invoke name="invokeBookInformationService" partnerLink="BookInformationServicePL" operation="getBookInfo" portType="ns3:BookInformationServicePortType" inputVariable="BookInformationServicePLRequest" outputVariable="BookInformationServicePLResponse"></bpel:invoke>
                        
                        <bpel:if name="IfBookQualityAvailable">
                            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$BookInformationServicePLResponse.parameters/ns3:return/book/status="yes"]]></bpel:condition>
                            <bpel:sequence>
                                <bpel:assign validate="no" name="assignToAddBook">
                                    <bpel:copy>
                                        <bpel:from>
                                        	<bpel:literal><ns:addOrderBook xmlns:ns="http://waa.a2.bookinventory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <ns:title></ns:title>
 <ns:author></ns:author>
 <ns:isbn></ns:isbn>
 <ns:publisher></ns:publisher>
 <ns:pdate>2017-05-12</ns:pdate>
</ns:addOrderBook></bpel:literal>
										</bpel:from>
                                        <bpel:to variable="BookInventoryServicePLRequest1" part="parameters"></bpel:to>
                                    </bpel:copy>
                                    
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/title]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookInventoryServicePLRequest1">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns2:title]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                    <bpel:copy>
