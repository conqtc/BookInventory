                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookInventoryServicePLRequest1">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns2:publisher]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/pdate]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookInventoryServicePLRequest1">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns2:pdate]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                </bpel:assign>
                                <bpel:invoke name="invokeBookInventoryService" partnerLink="BookInventoryServicePL" operation="addOrderBook" portType="ns2:BookInventoryServicePortType" outputVariable="BookInventoryServicePLResponse1" inputVariable="BookInventoryServicePLRequest1"></bpel:invoke>
                                <bpel:assign validate="no" name="assignFinalResult">
                                    <bpel:copy>
                                        <bpel:from>
                                        	<bpel:literal><ns:orderBookResponse xmlns:ns="http://www.example.org/BookOrderService/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<status>Successful</status>
<title></title>
<authors></authors>
<publisher></publisher>
</ns:orderBookResponse></bpel:literal>
										</bpel:from>
                                        <bpel:to variable="BookOrderPLResponse" part="parameters"></bpel:to>
                                    </bpel:copy>
                                    
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/title]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookOrderPLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[title]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/authors]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookOrderPLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[authors]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/publisher]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookOrderPLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[publisher]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                </bpel:assign>
                            </bpel:sequence>
