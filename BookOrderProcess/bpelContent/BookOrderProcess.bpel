<status>Error</status>
<title></title>
<authors></authors>
<publisher></publisher>
</ns:orderBookResponse></bpel:literal>
										</bpel:from>
                                        <bpel:to variable="BookOrderPLResponse" part="parameters"></bpel:to>
                                    </bpel:copy>
                                    <bpel:copy>
                                        <bpel:from part="parameters" variable="BookInformationServicePLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                                <![CDATA[ns3:return/book/title]]>
                                            </bpel:query>
                                        </bpel:from>
                                        <bpel:to part="parameters" variable="BookOrderPLResponse">
                                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[title]]></bpel:query>
                                        </bpel:to>
                                    </bpel:copy>
                                </bpel:assign>
                            </bpel:else>
                        </bpel:if>
                    </bpel:sequence>
                    <bpel:else>
                        <bpel:assign validate="no" name="assignBookAlreadyInDb">
                            <bpel:copy>
                                <bpel:from>
                                	<bpel:literal>
<ns:orderBookResponse xmlns:ns="http://www.example.org/BookOrderService/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<status>Error</status>
<title>Book already in database</title>
<authors></authors>
<publisher></publisher>
</ns:orderBookResponse>
									</bpel:literal>
								</bpel:from>
                                <bpel:to variable="BookOrderPLResponse" part="parameters"></bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                    </bpel:else>
                </bpel:if>
            </bpel:sequence>
            <bpel:else>
                <bpel:assign validate="no" name="assignStudentInvalid">
                    <bpel:copy>
                        <bpel:from>
                        	<bpel:literal>
<ns:orderBookResponse xmlns:ns="http://www.example.org/BookOrderService/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<status>Error</status>
<title>Invalid Student</title>
<authors></authors>
<publisher></publisher>
</ns:orderBookResponse>
							</bpel:literal>
						</bpel:from>
                        <bpel:to variable="BookOrderPLResponse" part="parameters"></bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
            </bpel:else>
        </bpel:if>
        <bpel:reply name="replyOrder" partnerLink="BookOrderPL" operation="orderBook" portType="ns:BookOrderServicePortType" variable="BookOrderPLResponse"></bpel:reply>
    </bpel:sequence>    
</bpel:process>

