        <bpel:assign validate="no" name="assignToValidateStudent">
            <bpel:copy>
                <bpel:from><bpel:literal><ns:validateStudent xmlns:ns="http://waa.a2.studentservice" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns:id>ns:id</ns:id>
