         xmlns:ns1="http://a2.waa/BookOrderArtifacts" xmlns:ns="http://www.example.org/BookOrderService/" xmlns:ns0="http://waa.a2.studentservice" xmlns:ns2="http://waa.a2.bookinventory" xmlns:ns3="http://waa.a2.bookinformation" xmlns:ns4="http://www.w3.org/2001/XMLSchema">

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:import namespace="http://waa.a2.studentservice" location="StudentService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
